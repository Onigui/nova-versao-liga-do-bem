{
  "expo": {
    "name": "Liga do Bem Botucatu",
    "slug": "liga-do-bem-botucatu",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#8B5CF6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ligadobem.botucatu"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#8B5CF6"
      },
      "package": "com.ligadobem.botucatu",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {},
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Este app precisa de acesso à localização para mostrar parceiros próximos."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Este app precisa de acesso à câmera para escanear QR codes."
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#8B5CF6",
          "defaultChannel": "default"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "73886b96-ebaa-4d18-b125-c8b3d53cc8ec"
      }
    },
    "owner": "onigui"
  }
}
